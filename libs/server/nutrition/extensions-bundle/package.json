{
	"name": "directus-extension-extensions-bundle",
	"version": "1.0.0",
	"type": "module",
	"directus:extension": {
		"host": "^10.1.9",
		"type": "bundle",
		"path": {
			"app": "../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle/app.js",
			"api": "../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle/api.js"
		},
		"entries": [
			{
				"type": "display",
				"name": "fdc-item",
				"source": "src/fdc-item/index.ts"
			},
			{
				"type": "endpoint",
				"name": "fdc",
				"source": "src/fdc/index.ts"
			}
		]
	},
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build && npm run copy-package && npm run replace-app && npm run replace-api",
		"dev": "directus-extension build -w --no-minify",
		"add": "directus-extension add",
		"copy-package": "copyfiles package.json ../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle",
		"replace-app": "replace-in-files ../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle/package.json --string ../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle/app.js --replacement app.js",
		"replace-api": "replace-in-files ../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle/package.json --string ../../../../dist/libs/server/nutrition/extensions/directus-extension-extensions-bundle/api.js --replacement api.js"
	}
}
