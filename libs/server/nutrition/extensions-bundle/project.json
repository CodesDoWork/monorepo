{
    "name": "server-nutrition-extensions-bundle",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/server/nutrition/extensions-bundle/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "nx:run-script",
            "options": {
                "script": "build"
            }
        },
        "dev": {
            "executor": "nx:run-script",
            "options": {
                "script": "dev"
            }
        },
        "add": {
            "executor": "nx:run-script",
            "options": {
                "script": "add"
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/server/nutrition/extensions-bundle/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/server/nutrition/extensions-bundle/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        }
    },
    "tags": []
}
