{
    "name": "just-music-downloader",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/just/music/downloader",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nx/next:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/packages/just/music/downloader",
                "debug": true
            }
        },
        "serve": {
            "executor": "@nx/next:server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "just-music-downloader:build",
                "dev": true
            },
            "configurations": {
                "development": {
                    "buildTarget": "just-music-downloader:build:development",
                    "dev": true
                },
                "production": {
                    "buildTarget": "just-music-downloader:build:production",
                    "dev": false
                }
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/just/music/downloader/jest.config.ts"
            }
        }
    },
    "tags": ["just-music-downloader"]
}
