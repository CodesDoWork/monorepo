{
    "$schema": "https://json-schema.org/schema",
    "$id": "EnvFiles",
    "title": "",
    "type": "object",
    "properties": {
        "stage": {
            "type": "string",
            "description": "Stage to use",
            "$default": {
                "$source": "argv",
                "index": 0
            },
            "x-prompt": "Which stage do you want to use?"
        },
        "username": {
            "type": "string",
            "description": "Username/Email for BW",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "What is your username?"
        },
        "password": {
            "type": "string",
            "description": "Password for BW",
            "$default": {
                "$source": "argv",
                "index": 2
            },
            "x-prompt": "What is your password?"
        }
    },
    "required": []
}
